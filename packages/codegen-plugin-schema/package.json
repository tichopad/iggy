{
  "name": "@iggy/codegen-plugin-schema",
  "version": "0.0.0",
  "packageManager": "yarn@3.0.1",
  "main": "dist/plugin.js",
  "private": true,
  "scripts": {
    "build": "yarn run --top-level esbuild src/plugin.ts --outdir=dist --tsconfig=tsconfig.build.json --format=cjs --minify --sourcemap",
    "start": "yarn run build --watch",
    "test": "yarn run --top-level jest"
  },
  "peerDependencies": {
    "graphql": "^15.5.3"
  },
  "yarn.build": {
    "input": "src",
    "output": "dist"
  }
}
